<prog> ::= <let-in-end> { <let-in-end> }
<let-in-end> ::= let <decl-list> in <type> ( <expr> ) end ;
<decl-list> ::= <decl> { <decl> }
<decl> ::= id : <type> = <expr> ;
<type> ::= int | real
<expr> ::= <term> { + <term> | - <term> } |
 if <cond> then <expr> else <expr>
<term> ::= <factor> { * <factor> | / <factor> }
<factor> ::= ( <expr> ) | id | number | <type> ( id )
<cond> ::= <oprnd> < <oprnd> |
 <oprnd> <= <oprnd> |
<oprnd> > <oprnd> |
<oprnd> >= <oprnd> |
<oprnd> == <oprnd> |
<oprnd> <> <oprnd>
<oprnd> ::= id | intnum